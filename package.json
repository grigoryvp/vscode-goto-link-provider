{
  "name": "goto-link-provider",
	"displayName": "goto link provider",
	"description": "Provides a 'goto://' link for advanced file navigation",
  "repository": "https://github.com/grigoryvp/vscode-goto-link-provider",
  "icon": "icon.png",
	"version": "0.1.0",
  "publisher": "grigoryvp",
  "license": "SEE LICENSE IN README.md",
	"engines": {
		"vscode": "^1.0.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
    "*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "goto-link-provider.open",
				"title": "Open deep link with goto-link-provider"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.66.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"eslint": "^8.11.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"typescript": "^4.5.5",
		"@vscode/test-electron": "^2.1.3"
	}
}
